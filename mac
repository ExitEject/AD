Attribute VB_Name = "Module1"
Sub goal2()
Attribute goal2.VB_ProcData.VB_Invoke_Func = " \n14"
'
' goal2 Macro
'

Dim workbookName As String
    Dim sheetName As String
    workbookName = ThisWorkbook.Name ' Gets the name of the workbook
    sheetName = ThisWorkbook.ActiveSheet.Name ' Gets the name of the active worksheet

    Columns("A:O").Select
    Selection.Delete Shift:=xlToLeft
    Columns("B:D").Select
    Columns("E:E").ColumnWidth = 18.29
    Selection.Delete Shift:=xlToLeft
    Columns("C:G").Select
    Selection.Delete Shift:=xlToLeft
    Rows("1:1").Select
    Selection.Delete Shift:=xlUp
    Columns("A:A").Select
    workbookname.sheetname.Sort.SortFields.Clear
        workbookname.sheetname..Sort.SortFields.Add2 Key:=Range _
        ("A1:A575"), SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:= _
        xlSortNormal
    With workbookname.sheetname.Sort
        .SetRange Range("A1:B575")
        .Header = xlNo
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    Rows("1:1").Select
    Selection.Insert Shift:=xlDown, CopyOrigin:=xlFormatFromLeftOrAbove
    Range("A1").Select
    ActiveCell.FormulaR1C1 = "Owner"
    Range("B1").Select
    ActiveCell.FormulaR1C1 = "Shepherd_Issue_Id"
    Columns("B:B").Select
    Selection.AutoFilter
    ActiveSheet.Range("$B$1:$B$577").AutoFilter Field:=1, Criteria1:="<>"
    ActiveSheet.Range("$A$1:$B$577").RemoveDuplicates Columns:=2, Header:=xlNo
    Range("C3").Select
    ActiveCell.FormulaR1C1 = "=CONCAT(""https://shepherd.a2z.com/issues/"",RC[-1])"
    Range("C3").Select
    Selection.FillDown
    Range("C1").Select
    ActiveCell.FormulaR1C1 = "Shepherd_Issue_ID"
    Range("D1").Select
    ActiveCell.FormulaR1C1 = "VMR OWNER"
    Range("E1").Select
    ActiveCell.FormulaR1C1 = "Notes"
    Range("F1").Select
    ActiveCell.FormulaR1C1 = "Healthy"
    Range("G1").Select
    Columns("C:C").EntireColumn.AutoFit
    Columns("D:D").EntireColumn.AutoFit
End Sub
